{
  "compilerOptions": {
    "target": "ES6",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "jsx": "react",
    "rootDirs": [
      "src"
    ],
    "baseUrl": "src",
    "paths": {
      "@/*": ["*"]
    },
    "allowJs": true,
    "strict": true,
    "resolveJsonModule": true,
  },
  "include": [
    "src"
  ],
  "exclude": []
}